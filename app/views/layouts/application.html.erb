<!DOCTYPE html>
<html>
  <head>
    <title>MaMazon</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <link href="https://fonts.googleapis.com/css2?family=Cabin+Sketch:wght@400;700&family=Corinthia:wght@700&family=Dongle&family=Redacted+Script:wght@300&family=Rock+3D&display=swap" rel="stylesheet">
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type="text/javascript">
    window.mamazonURL = "<%= image_url('MAMAZON.png') %>";
    window.homelogoURL = "<%= image_url('homelogo.png') %>"
    window.cartURL = "<%= image_url('carticon.png') %>"
    window.avatarURL = "<%= image_url('avatar.png') %>"
    window.leftURL = "<%= image_url('left.png') %>"
    window.rightURL = "<%= image_url('right.png') %>"
    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
